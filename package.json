{
  "name": "mrmagnet",
  "productName": "Mr. Magnet",
  "main": "main.js",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "electron": "ng build --prod --watch & electron .",
    "ng": "ng",
    "package-mac": "ng build --prod && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/Mr.\\ Magnet-darwin-x64/Mr.\\ Magnet.app Mr.Magnet --out=release-builds --overwrite --icon=src/assets/icons/mac/icon.icns --background=src/assets/background.png",
    "package-win": "ng build --prod && electron-packager . mrmagnet --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Mr. Magnet\"",
    "package-linux": "ng build --prod && electron-packager . mrmagnet --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@types/lodash": "^4.14.78",
    "bittorrent-protocol": "^2.2.3",
    "bittorrent-tracker": "^9.2.3",
    "core-js": "^2.4.1",
    "electron-store": "^1.3.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "rutracker-api": "^0.2.0",
    "rxjs": "^5.4.2",
    "torrent-discovery": "^8.3.1",
    "uikit": "^3.0.0-beta.30",
    "ut_pex": "^1.1.6",
    "webtorrent": "^0.98.20",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "electron": "^1.7.9",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
